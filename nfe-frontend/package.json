{
  "name": "nw-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --env=dev",
    "build-dev": "ng build --env=dev",
    "build-qai": "ng build --env=qai",
    "build-uat": "ng build --env=uat",
    "start-mock": "ng serve --env=mock",
    "test": "ts-node setkarma && ng test --watch=false --cc",
    "e2e": "ng e2e --config protractor.conf.js --env=mock",
    "e2e-jenkins": "ng e2e --config protractor.jenkins.conf.js --env=mock --silent",
    "coverage": "ng test --watch=false --single-run --cc",
    "lint": "ng lint",
    "json-server": "node json-server.js",
    "mock": "concurrently --kill-others \"npm run json-server\" \"npm run start-mock\"",
    "protractor": "concurrently --success first --kill-others \"npm run json-server\" \"npm run e2e\"",
    "protractor-jenkins": "concurrently --success first --kill-others \"npm run json-server\" \"npm run e2e-jenkins\""
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "angular-l10n": "^4.1.4",
    "bootstrap": "^4.1.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "jquery": "^3.3.1",
    "jqwidgets-scripts": "5.7.2",
    "ng2-logger": "1.0.4",
    "ngx-cookie-service": "^1.0.10",
    "primeng": "^5.2.4",
    "rxjs": "^5.5.6",
    "yarn": "^1.7.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.2",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.1",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "5.3.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "concurrently": "^3.5.0",
    "json-server": "^0.12.1"
  }
}
